		<script>
		var f=1;
			function change(){
						
				if(f == 1)
				{document.getElementById("mainframe").style.backgroundImage = 'url(eKvzqL6.jpg)';
				 f=2;
				}
				else if(f == 2)
				{document.getElementById("mainframe").style.backgroundImage = 'url(food.jpg)';
				f=1;
				}
				
				
			
			}
			
			setInterval(function(){change();},3000);
		
		</script>
		
